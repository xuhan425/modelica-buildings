        T=283.15) annotation (Placement(transformation(extent={{40,-54},{60,-34}})));
      Fluid.Sources.FixedBoundary bouOut(nPorts=1,redeclare package Medium =
            MediumA)
        annotation (Placement(transformation(extent={{40,-84},{60,-64}})));
    equation
