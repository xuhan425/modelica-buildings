      connect(multiplex3_1.y,roo. qGai_flow) annotation (Line(
          points={{41,-10},{78.4,-10}},
          color={0,0,127},
          smooth=Smooth.None));
      connect(weaDat.weaBus,roo. weaBus) annotation (Line(
          points={{160,90},{176,90},{176,-0.1},{117.9,-0.1}},
          color={255,204,51},
          thickness=0.5,
